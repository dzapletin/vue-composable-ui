{
  "name": "vue-composable-ui",
  "version": "1.0.7",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzapletin/vue-composable-ui.git"
  },
  "main": "./dist/composable-ui.umd.cjs",
  "module": "./dist/composable-ui.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/main.d.ts",
        "default": "./dist/composable-ui.js"
      },
      "require": {
        "types": "./dist/types/main.d.ts",
        "default": "./dist/composable-ui.umd.cjs"
      }
    }
  },
  "types": "./dist/types/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "glob": "^11.0.2",
    "release-it": "^19.0.4",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "vite": "^7.0.6",
    "vitepress": "^1.6.4",
    "vue": "^3.5.12",
    "vue-tsc": "^3.0.5"
  }
}
